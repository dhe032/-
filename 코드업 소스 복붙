//1292ë²ˆ
#include <stdio.h>
int main(){
	int a,b=0;
	scanf("%d",&a);
	for(int i=0;i<10;i++){
		b+=a%10;
		a=a/10;
	}
	if(b%7==4){
		printf("suspect");
	}
	else{
		printf("citizen");
	}
}


//1290
#include<stdio.h>
int main()
{
	int n, c=0, i;
	scanf("%d", &n);
	for(i=1;i<n;i++)
	if(n%i==0)
		c++;
	printf("%d",c);
}

//1712
#include <stdio.h>

int main() {
  int i,a,b,c,d,e,f;
  scanf("%d %d %d",&a,&b,&c);
  d=b-a;
  for(i=a;i<=c;i+=d){
    printf("%d ",i);
  }
}

//1714
#include <stdio.h>

int main() {
	int a;
	scanf("%d", &a);
	do{
		printf("%d", a%10);
		a=a/10;
	} while(a>0);
	return 0;
}

//1732
#include <stdio.h>
#include<math.h>
int main(){
    int a,b,d,e;
    float c;
    scanf("%d %d",&a ,&b);
    e = pow(a,b);
    printf("%.1f\n", (float)a / b);
    printf("%d\n", (int)(a / b));
    printf("%d",e);
}

//1733
#include <stdio.h>
int main() {
	char c[100];
	gets(c);
	if ((int)c[0]==73 && (int)c[1]==79 && (int)c[2]==73 && c[3]=='\0')
		printf("IOI is the International Olympiad in Informatics.");
	else
		printf("I don't care.");
	return 0;
}

//1734
#include "stdio.h"
int main()
{
    char a[100];
    gets(a);
    printf("welcome! ");
    for(int i=0; a[i]!='\0'; i++){
        printf("%c",a[i]);
    }
    return 0;
}

//1735
#include <stdio.h>
int main(){
	int n;
	scanf("%d", &n);
	for(int i=1;i<=n;i++){
		printf("hello\n");
	}
	return 0;
}

//1736
#include <stdio.h>

int main()
{
	int a;

	scanf("%d", &a);
	printf("%d %d %d %d",a/86400 ,a%86400 / 3600, a % 3600 / 60, a % 3600 % 60);
	return 0;
}

//1231
#include <stdio.h>
 
int main ()
{
    int a,b;
    char c;
    
    scanf("%d%c%d",&a,&c,&b);
    
    if(c=='+'){
        printf("%d",a + b);
    }
    else if(c=='-'){
        printf("%d",a-b);
    }
    else if(c=='*'){
        printf("%d",a*b);
    }
    else{
        printf("%.02f",(float)a/(float)b);
    }
    return 0;
}

